    "@choseohwan/jest-preset-base": "^1.0.0",
    "@choseohwan/rollup-builder": "^1.1.4",
    "@choseohwan/rollup-plugin-builder-base": "^1.0.1",
    "@choseohwan/tsconfig-base": "^1.0.11"
  },
  "lint-staged": {
    "*.(js|jsx|ts|tsx)": [
