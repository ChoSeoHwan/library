  "directories": {
    "src": "src",
    "dist": "dist"
  },
  "files": [
    "dist/*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ChoSeoHwan/library.git"
