{
  "name": "@choseohwan/utils",
  "version": "1.0.13-beta.0",
  "description": "typescript utility functions",
  "keywords": [
    "utils",
    "utilities"
  ],
  "author": "wtae1216 <wtae1216@gmail.com>",
  "homepage": "https://github.com/ChoSeoHwan/library#readme",
  "license": "ISC",
  "main": "dist/esm/index.js",
  "types": "dist/esm/src/index.d.ts",
  "exports": {
    "./": "./dist/esm/index.js",
    "./prototypes": "./dist/esm/prototypes.js"
  },
  "typesVersions": {
    "*": {
      "prototypes": [
        "dist/esm/src/prototypes/index.d.ts"
      ]
    }
  },
  "directories": {
    "src": "src",
    "dist": "dist"
  },
  "files": [
    "dist/*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ChoSeoHwan/library.git"
  },
  "scripts": {
    "eslint": "eslint './src/**/*.ts'",
    "eslint:fix": "yarn eslint --fix",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "prepublish": "yarn build",
    "lint-staged": "lint-staged",
    "build": "rollup -c rollup.config.js",
    "precommit": "yarn lint-staged"
  },
  "bugs": {
    "url": "https://github.com/ChoSeoHwan/library/issues"
  },
  "devDependencies": {
    "@choseohwan/eslint-config-base": "^1.0.7",
    "@choseohwan/jest-config-base": "^1.0.3",
    "@choseohwan/prettier-config-base": "^1.0.3",
    "@choseohwan/rollup-builder": "^1.0.28",
    "@choseohwan/tsconfig-base": "^1.0.7"
  },
  "lint-staged": {
    "*.(js|jsx|ts|tsx)": [
      "yarn eslint:fix",
      "git add"
    ]
  }
}
