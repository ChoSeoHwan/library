{
  "name": "@choseohwan/react-utils",
  "version": "2.0.1-temp.0",
  "description": "react utils for SeoHwan Cho's project",
  "keywords": [
    "react",
    "utils"
  ],
  "author": "ChoSeoHwan <wtae1216@naver.com>",
  "homepage": "https://github.com/ChoSeoHwan/library#readme",
  "license": "ISC",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.mjs",
  "types": "dist/cjs/src/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.js"
    },
    "./hoc": {
      "import": "./dist/esm/hoc.mjs",
      "require": "./dist/cjs/hoc.js"
    },
    "./hooks": {
      "import": "./dist/esm/hooks.mjs",
      "require": "./dist/cjs/hooks.js"
    }
  },
  "typesVersions": {
    "*": {
      "hoc": [
        "dist/esm/src/hoc/index.d.ts"
      ],
      "hooks": [
        "dist/esm/src/hooks/index.d.ts"
      ]
    }
  },
  "directories": {
    "src": "src"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ChoSeoHwan/library.git"
  },
  "scripts": {
    "eslint": "eslint \"./src/**/*.ts?(x)\"",
    "eslint:fix": "yarn eslint --fix",
    "lint-staged": "lint-staged",
    "test": "jest --config=jest/jest.config.ts",
    "test:silent": "yarn test --silent",
    "test:watch": "yarn test --watchAll",
    "prepublish": "yarn eslint && yarn test:silent && yarn build",
    "build": "rollup -c rollup.config.js --environment NODE_ENV:production",
    "precommit": "yarn lint-staged"
  },
  "bugs": {
    "url": "https://github.com/ChoSeoHwan/library/issues"
  },
  "dependencies": {
    "@types/throttle-debounce": "^5.0.0",
    "react": "^18.2.0",
    "throttle-debounce": "^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.19.1",
    "@babel/plugin-transform-runtime": "^7.19.1",
    "@babel/preset-env": "^7.19.1",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@choseohwan/babel-preset-react": "^2.0.1-temp.0",
    "@choseohwan/eslint-config-react": "^2.0.1-temp.0",
    "@choseohwan/jest-preset-base": "^2.0.1-temp.0",
    "@choseohwan/rollup-builder": "^2.0.1-temp.0",
    "@choseohwan/rollup-plugin-builder-base": "^2.0.1-temp.0",
    "@choseohwan/tsconfig-base": "^2.0.0-temp.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@tsconfig/recommended": "^1.0.1",
    "@types/jest": "^29.0.3",
    "@types/node": "^18.7.18",
    "@types/react": "^18.0.20",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "babel-jest": "^29.0.3",
    "eslint": "8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^39.3.6",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "jest": "^29.0.3",
    "jest-environment-jsdom": "^29.0.3",
    "jest-html-reporters": "^3.0.11",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "react-dom": "^18.2.0",
    "rollup": "^2.79.0",
    "ts-jest": "^29.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3",
    "typescript-transform-paths": "^3.3.1"
  },
  "peerDependencies": {
    "react": ">=18"
  },
  "lint-staged": {
    "*.(js|jsx|ts|tsx)": [
      "yarn eslint:fix"
    ]
  },
  "gitHead": "1cb63c3e587affe95ed1f984abbfe1ae60490a8c"
}
